{
  "name": "md-localify",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json && esbuild ./src/prod.ts --bundle --minify --platform=node --outfile=./dist/bundle.cjs --format=cjs --tsconfig=./tsconfig.json",
    "start": "node ./dist/bundle.cjs",
    "dev": "tsx --env-file=.env ./src/dev.ts dev",
    "dev:generate": "tsx --env-file=.env ./src/dev.ts build",
    "dev:core": "tsx watch --env-file=.env --include=\"./*\" ./src/index.ts dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^5.0.5",
    "@types/helmet": "^4.0.0",
    "@types/imagemin": "^9.0.1",
    "@types/imagemin-gifsicle": "^7.0.4",
    "@types/imagemin-mozjpeg": "^8.0.4",
    "@types/imagemin-pngquant": "^8.0.0",
    "@types/imagemin-svgo": "^11.0.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.9.2",
    "@types/sanitize-html": "^2.16.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "dompurify": "^3.3.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "imagemin": "^9.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "marked": "^16.4.1",
    "sanitize-html": "^2.17.0"
  }
}
